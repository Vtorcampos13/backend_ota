extends ../layout

block content 
    if error 
        p(class="error", style="color:red;")= error
    if coche 
        article 
            h1="marca: "+coche.marca
            p="modelo: "+coche.modelo
            p="matricula: "+coche.matricula
            button 
                a(href='/parking/new')
            button 
                a(href='/coches/'+coche.id_coche+'/edit') Editar
            button
                a(href='/coches/'+coche.id_coche+'/delete') Eliminar